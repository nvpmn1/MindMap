# Server
PORT=3001
NODE_ENV=development
ALLOW_PROFILE_AUTH=true
DEFAULT_WORKSPACE_ID=11111111-1111-1111-1111-111111111111

# Supabase (service role - NEVER expose to frontend!)
SUPABASE_URL=https://your-project.supabase.co
SUPABASE_SERVICE_ROLE_KEY=your-service-role-key
SUPABASE_ANON_KEY=your-anon-key

# Claude API (NEVER expose to frontend!)
CLAUDE_API_KEY=your-claude-api-key
CLAUDE_MODEL=claude-haiku-4-5-20251001
CLAUDE_MAX_TOKENS=8192
CLAUDE_TEMPERATURE=0.9

# CORS
FRONTEND_URL=http://localhost:5173
# Optional: comma-separated production origins
# CORS_ORIGIN=https://mindmap-hub.vercel.app

# Rate Limiting
RATE_LIMIT_WINDOW_MS=60000
RATE_LIMIT_MAX=100
AI_RATE_LIMIT_MAX=10
