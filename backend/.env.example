# Server
PORT=3001
NODE_ENV=development
ALLOW_PROFILE_AUTH=true
DEFAULT_WORKSPACE_ID=11111111-1111-1111-1111-111111111111

# Supabase (service role - NEVER expose to frontend!)
SUPABASE_URL=https://your-project.supabase.co
SUPABASE_SERVICE_ROLE_KEY=your-service-role-key

# Claude API (NEVER expose to frontend!)
CLAUDE_API_KEY=your-claude-api-key
CLAUDE_MODEL=claude-sonnet-4-5
CLAUDE_MAX_TOKENS=4096
CLAUDE_TEMPERATURE=0.7
CLAUDE_MODEL_STRATEGY=cheap

# CORS
FRONTEND_URL=http://localhost:5173
# Optional: comma-separated list of additional allowed origins
# CORS_ORIGINS=https://app.example.com,https://staging.example.com

# Rate Limiting
RATE_LIMIT_WINDOW_MS=60000
RATE_LIMIT_MAX=100
AI_RATE_LIMIT_MAX=10
